# API Keys
# Get your Google API key from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key

# Optional: OpenAI API key (if you want to use OpenAI instead)
OPENAI_API_KEY=your_openai_api_key_here

# Model Provider (gemini or openai)
# Default: gemini (free tier available)
MODEL_PROVIDER=gemini

# Server Configuration
PORT=3001
NODE_ENV=development

# LangSmith (Optional - for debugging and tracing)
# Set to "true" to enable LangSmith tracing
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=ai-agent
